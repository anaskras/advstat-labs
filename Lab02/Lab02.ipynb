   ],
   "source": [
    "def pearsonr(x, y):\n",
    "    n = len(x)\n",
    "    x_mean = np.mean(x)\n",
    "    y_mean = np.mean(y)\n",
    "    x_meaned = [p - x_mean for p in x]\n",
    "    y_meaned = [p - y_mean for p in y]\n",
    "    x_meaned_sq = [p ** 2 for p in x_meaned]\n",
    "    y_meaned_sq = [p ** 2 for p in y_meaned]\n",
    "    corr_pearson = np.correlate(x_meaned, y_meaned) / sqrt(sum(x_meaned_sq) * sum(y_meaned_sq))\n",
    "    \n",
    "    t = corr_pearson * sqrt((n - 2) / (1 - pow(corr_pearson, 2)))\n",
    "\n",
    "print(pearsonr(data['Height'], data['Weight']))"
   ]
  },

   ],
   "source": [
    "# compute demeaned correlation divided by n-1 using scipy.signal\n",
    "h_mean = np.mean(data['Height'])\n",
    "h_meaned = [x - h_mean for x in data['Height']]\n",
    "w_mean = np.mean(data['Weight'])\n",
    "w_meaned = [x - w_mean for x in data['Weight']]\n",
    "hw_corr = correlate(h_meaned, w_meaned, 'valid') / (len(h_meaned) - 1)\n",
    "print(hw_corr)"
   ]
